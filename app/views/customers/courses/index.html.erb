<p style="color: green"><%= notice %></p>
<h1>Courses</h1>
<div id="courses">
  <% @courses.each do |course| %>
    <%= render 'customers/courses/partial/course', course: course %>
    <p>
      <%= link_to "Show this course", customers_course_path(course) %>
    </p>
  <% end %>
</div>
<%= link_to "New course", new_customers_course_path %>
